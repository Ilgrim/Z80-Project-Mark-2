               00048 ;===============================================================================
               00049 ; hostbus_init - Setup the PSP and enable relevant interrupts
               00050 ;===============================================================================
               00051 
               00052 ;; Called at boot time, sets the port directions and interrupts for running the
               00053 ;; parallel slave port and interrupt outputs.
               00054 
0000 00055 hostbus_init
               00056   ; setup the pointers, INDF0 is write pointer, INDF1 is read, make a looping
               00057   ; 256 byte fifo
0000 0E01      00058   movlw         0x01
0002 6EEA      00059   movwf         FSR0H
0004 6EE2      00060   movwf         FSR1H
0006 6AE9      00061   clrf          FSR0L
0008 6AE1      00062   clrf          FSR1L
               00063 
               00064   ; clear the PSPIF flag if it's set then enable the interrupt
000A 9E9E      00065   bcf           PIR1, PSPIF
000C 8E9D      00066   bsf           PIE1, PSPIE
000E 0012      00067   return
               00068 
               00069 ;===============================================================================
               00070 ; hostbus_int - Interrupt service routine for the PSP module
               00071 ;===============================================================================
               00072 
               00073 ;; Called when PSPIF triggers an interrupt.  Deals with commands from the Z80
               00074 ;; and keeping the output buffer loaded with the latest queued byte from the
               00075 ;; FIFO.
               00076 
0010 00077 hostbus_int
               00078   ; test to see if the Z80 read or wrote, if write IBF will be set (OBF might
               00079   ; be low but could just mean we don't have anything to send)
0010 BE96      00080   btfsc         TRISE, IBF
0012 D000      00081   bra           hostbus_int_write       ; IBF set, so Z80 wrote a byte
               00082   ; otherwise Z80 read a byte
0014 00083 hostbus_int_read
               00084   ; a byte has been read by the Z80
               00085   ; see if there is a new byte and load that into the buffer
               00086   ; first clear the interrupt so that the Z80 doesn't come back immediately
0014 8A89      00087   bsf           LATA, P_INT
               00088 
0016 9E9E      00089   bcf           PIR1, PSPIF             ; clear the internal interrupt flag
0018 50E9      00090   movf          FSR0L,w
001A 18E1      00091   xorwf         FSR1L,w
001C B4D8      00092   btfsc         STATUS, Z               ; if low bytes of fifo write and read
001E 0012      00093   return                                ; match, there's no new data
               00094   ; if not then there's more data for the Z80
0020 50E6      00095   movf          POSTINC1,w              ; get the byte from the read pointer
0022 6E83      00096   movwf         PORTD                   ; put it in the buffer for the next read
0024 0E01      00097   movlw         0x01
0026 6EE2      00098   movwf         FSR1H                   ; make sure we don't overflow the buffer
0028 9A89      00099   bcf           LATA, P_INT             ; set the interrupt again
