02E0 00581 key_nd_decode
               00582   ; return a keycode in the custom code map specified in scancodes.inc
               00583   ; this sets bit 7 if the key is a control (escape - F1-F12 - Home, Insert etc.)
               00584   ; bits 7 and 6 are set if it is a numberpad key
               00585   ; if neither bit 6 or 7 are set it's a main character/number key
02E0 6EF6      00586   movwf         TBLPTRL
02E2 6AF8      00587   clrf          TBLPTRU
02E4 0E77      00588   movlw         0x77
02E6 6EF7      00589   movwf         TBLPTRH
02E8 0008      00590   tblrd*
02EA 50F5      00591   movf          TABLAT,w
02EC 0012      00592   return
