0008 00075 interrupt
0008 BE9E      00076   btfsc         PIR1, PSPIF
000A EF00 F000 00077   goto          psp_rd_int
000E B4F2      00078   btfsc         INTCON, TMR0IF
0010 EF00 F000 00079   goto          timeout
gpasm-0.13.7 beta               main.asm    2-16-2010  22:07:25          PAGE  5


LOC  OBJECT CODE     LINE SOURCE TEXT
  VALUE
 
0014 EF00 F000 00080   goto          edge_event
               00081 
0018 00082 psp_rd_int
               00083   ; a byte has been read by the Z80
               00084   ; see if there is a new byte and load that into the buffer
               00085   ; first clear the interrupt so that the Z80 doesn't come back immediately
0018 8A89      00086   bsf           LATA, P_INT
               00087 
001A 9E9E      00088   bcf           PIR1, PSPIF             ; clear the interrupt flag
001C 50E9      00089   movf          FSR0L,w
001E 18E1      00090   xorwf         FSR1L,w
0020 E000      00091   bz            exit_interrupt          ; if low bytes of fifo write and read
               00092                                         ; match, there's no new data
               00093   ; if not then there's more data for the Z80
0022 50E6      00094   movf          POSTINC1,w              ; get the byte from the read pointer
0024 6E83      00095   movwf         PORTD                   ; put it in the buffer for the next read
0026 0E01      00096   movlw         0x01
0028 6EE2      00097   movwf         FSR1H                   ; make sure we don't overflow the buffer
002A 9A89      00098   bcf           LATA, P_INT             ; set the interrupt again
002C EF00 F000 00099   goto          exit_interrupt
               00100 
0030 00101 timeout
               00102   ; called if Timer0 overflows.  This means that there wasn't a bit edge in time
               00103   ; alternatively that we've initiated host->device comms
0030 BA00      00104   btfsc         flags, talking_flag
0032 EF00 F000 00105   goto          timeout_talking
               00106   ; just a timeout.  Set all registers back to zero
0036 6A00      00107   clrf          bit_count
0038 6A00      00108   clrf          rxreg
003A 9ED5      00109   bcf           T0CON, TMR0ON           ; disable the timeout until the next
               00110                                         ; start bit
003C 94F2      00111   bcf           INTCON, TMR0IF
003E EF00 F000 00112   goto          exit_interrupt
               00113 
0042 00114 timeout_talking
               00115   ; Timer0 overflowed, but this is the start of the talking routine
0042 0E00      00116   movlw         0x00
0044 1800      00117   xorwf         bit_count,w
0046 E000      00118   bz            timeout_start_talking
               00119   ; it's a timeout in some other part of the talk routine.  set send_failed_flag
               00120   ; and clear up
0048 8C00      00121   bsf           flags, send_failed_flag
004A 9A00      00122   bcf           flags, talking_flag
004C 9ED5      00123   bcf           T0CON, TMR0ON           ; disable the timer again
004E 8293      00124   bsf           TRISB,1                 ; make sure the data pin is an input!
0050 6A00      00125   clrf          bit_count
0052 6A00      00126   clrf          txreg
0054 94F2      00127   bcf           INTCON, TMR0IF          ; clear the interrupt flag
0056 EF00 F000 00128   goto          exit_interrupt          ; all done
               00129 
005A 00130 timeout_start_talking
               00131   ; just finished the "I want to talk signal"
               00132   ; initialise the parity counter for odd parity
005A 8000      00133   bsf           parity, parity_flag
gpasm-0.13.7 beta               main.asm    2-16-2010  22:07:25          PAGE  6


LOC  OBJECT CODE     LINE SOURCE TEXT
  VALUE
 
               00134   ; set data as output
005C 9293      00135   bcf           TRISB, 1
005E 0E14      00136   movlw         d'20'
0060 6E00      00137   movwf         count
0062 00138 timeout_wait_loop
0062 2E00      00139   decfsz        count,f
0064 D000      00140   bra           timeout_wait_loop
               00141   ; set clock as input
0066 8093      00142   bsf           TRISB, 0
               00143   ; clear the edge interrupt flag
0068 92F2      00144   bcf           INTCON, INT0IF
               00145   ; enable the edge interrupt again
006A 88F2      00146   bsf           INTCON, INT0IE
               00147   ; increment the bit counter because that data bit is the start bit
006C 2A00      00148   incf          bit_count,f
               00149   ; re-enable the timer overflow interrupt
006E 94F2      00150   bcf           INTCON, TMR0IF
0070 9ED5      00151   bcf           T0CON, TMR0ON
0072 6AD6      00152   clrf          TMR0L
               00153 ;   clrf          txreg
               00154   ; leave the interrupt
0074 EF00 F000 00155   goto          exit_interrupt
               00156 
0078 00157 edge_event
               00158   ; a falling edge on the keyboard clock save the bit
0078 8ED5      00159   bsf           T0CON, TMR0ON           ; enable timeout counter
007A 0E00      00160   movlw         d'0'
007C 1800      00161   xorwf         bit_count,w
007E E000      00162   bz            edge_event_start
0080 0E09      00163   movlw         d'9'
0082 1800      00164   xorwf         bit_count,w
0084 E000      00165   bz            edge_event_parity
0086 0E0A      00166   movlw         d'10'
0088 1800      00167   xorwf         bit_count,w
008A E000      00168   bz            edge_event_stop
008C 0E0B      00169   movlw         d'11'                   ; ack bit in tx mode only
008E 1800      00170   xorwf         bit_count,w
0090 E000      00171   bz            edge_event_ack
               00172 
               00173   ; it's a data bit
               00174   ; see if we're in talk or listen mode
0092 BA00      00175   btfsc         flags, talking_flag
0094 D000      00176   bra           edge_event_send_bit
               00177   ; listening so clock in a bit
0096 4200      00178   rrncf         rxreg,f
0098 9E00      00179   bcf           rxreg,7
009A B281      00180   btfsc         PORTB, 1
009C 8E00      00181   bsf           rxreg,7
009E EF00 F000 00182   goto          edge_event_exit
               00183 
00A2 00184 edge_event_send_bit
00A2 B000      00185   btfsc         txreg,0
00A4 8293      00186   bsf           TRISB, 1        ; only one of these will run but it avoids
00A6 A000      00187   btfss         txreg,0         ; 'glitching' the hardware line with a clear-set
gpasm-0.13.7 beta               main.asm    2-16-2010  22:07:25          PAGE  7


LOC  OBJECT CODE     LINE SOURCE TEXT
  VALUE
 
00A8 9293      00188   bcf           TRISB, 1
00AA B000      00189   btfsc         txreg,0
00AC 7000      00190   btg           parity,parity_flag  ; don't forget the parity bit
00AE 4200      00191   rrncf         txreg,f         ; rotate the register to put the next bit at 0
00B0 EF00 F000 00192   goto          edge_event_exit ; all done
               00193 
00B4 00194 edge_event_stop
               00195   ; if we got this far it's the stop bit
               00196   ; see if we need to receive or send
00B4 BA00      00197   btfsc         flags, talking_flag
00B6 D000      00198   bra           edge_event_send_stop
00B8 9ED5      00199   bcf           T0CON, TMR0ON           ; turn off the timer
00BA 6800      00200   setf          bit_count               ; clear the bit count for the next one
00BC EF00 F000 00201   goto          key_translate
               00202   ; do some translation here
               00203 
00C0 00204 edge_event_send_stop
               00205   ; need to send the stop bit
00C0 8293      00206   bsf           TRISB, 1                ; set data as HIGH-Z
               00207   ; not quite done, need to wait for an ack as well
00C2 EF00 F000 00208   goto          edge_event_exit
               00209 
00C6 00210 edge_event_save_byte
00C6 BC96      00211   btfsc         TRISE,OBF
00C8 D000      00212   bra           edge_event_load_fifo
               00213   ; the buffer is empty so load the byte directly to the Z80
               00214 ;   movf          rxreg,w
00CA 6E83      00215   movwf         PORTD
00CC 9A89      00216   bcf           LATA,P_INT              ; set an interrupt.
00CE EF00 F000 00217   goto          edge_event_exit
               00218 
00D2 00219 edge_event_load_fifo
               00220 ;   movf          rxreg,w
00D2 6EEE      00221   movwf         POSTINC0                ; bung the byte in the fifo
00D4 0E01      00222   movlw         0x01
00D6 6EEA      00223   movwf         FSR0H                   ; make sure we wrap around 256 bytes
00D8 EF00 F000 00224   goto          edge_event_exit
               00225 
00DC 00226 edge_event_start
               00227   ; only do this on receive
00DC 6A00      00228   clrf          rxreg
00DE 8ED5      00229   bsf           T0CON, TMR0ON           ; enable the timeout function
00E0 EF00 F000 00230   goto          edge_event_exit
               00231 
00E4 00232 edge_event_parity
               00233   ; only care when we're talking but need to check this
00E4 AA00      00234   btfss         flags, talking_flag
00E6 EF00 F000 00235   goto          edge_event_exit
               00236   ; okay so we're talking, send the parity bit
00EA A000      00237   btfss         parity, parity_flag
00EC 9293      00238   bcf           TRISB,1
00EE B000      00239   btfsc         parity, parity_flag
00F0 8293      00240   bsf           TRISB,1
               00241   ; all done
gpasm-0.13.7 beta               main.asm    2-16-2010  22:07:25          PAGE  8


LOC  OBJECT CODE     LINE SOURCE TEXT
  VALUE
 
00F2 EF00 F000 00242   goto          edge_event_exit
               00243 
00F6 00244 edge_event_ack
               00245   ; this is the acknowledge from the keyboard when we've just sent a byte
00F6 B281      00246   btfsc         PORTB,1
00F8 D000      00247   bra           edge_event_ack_error    ; should have been low
               00248   ; all done transmitting, clear the flags and tidy up
00FA 9ED5      00249   bcf           T0CON, TMR0ON           ; stop the timer
00FC 6800      00250   setf          bit_count               ; reset the bit counter
00FE 9A00      00251   bcf           flags, talking_flag     ; exit talking mode
0100 6A00      00252   clrf          txreg
0102 EF00 F000 00253   goto          edge_event_exit
               00254 
0106 00255 edge_event_ack_error
               00256   ; all went wrong somewhere, set send_failed bit and clear the status
0106 8C00      00257   bsf           flags, send_failed_flag
0108 9A00      00258   bcf           flags, talking_flag
010A 6800      00259   setf          bit_count
010C 9ED5      00260   bcf           T0CON, TMR0ON
010E 6A00      00261   clrf          txreg
0110 EF00 F000 00262   goto          edge_event_exit
               00263 
0114 00264 edge_event_exit
0114 2A00      00265   incf          bit_count,f
0116 6AD6      00266   clrf          TMR0L                   ; clear the timeout counter
0118 92F2      00267   bcf           INTCON, INT0IF
011A EF00 F000 00268   goto          exit_interrupt
               00269 
011E 00270 exit_interrupt
011E 0011      00271   retfie        FAST
               00272 
               00273 ; == End of Interrupt code ====================================================
               00274 
