                                           RX_COUNT        RES     1
                                           RX_CHECKSUM     RES     1
                                           RX_FLAGS        RES     1
                                           ; -- FLAG VALUES --------------------------------------------------------------
                                           CHECKSUM_FAIL   EQU     0x01
                                           
                                           TX_CHECKSUM     RES     1
                                           TX_COUNT        RES     1
                                           JUMP_REG        RES     1
                                             
