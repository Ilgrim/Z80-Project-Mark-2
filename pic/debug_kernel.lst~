                                           RX_MODE RES     1
                                           ; -- RX MODE CONSTANTS --------------------------------------------------------
                                           RX_COM  EQU     0
                                           RX_LEN  EQU     1
                                           RX_DAT  EQU     2
                                           RX_CKS  EQU     3
                                           RX_BSY  EQU     4
                                           
