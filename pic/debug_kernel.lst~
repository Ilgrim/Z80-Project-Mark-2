                                           ; -- serial command functions -------------------------------------------------
                                           
                                           serial_read_io
                                               ; read a single IO port to the serial host
                                               ; Address   2 bytes be
                                               ; check that the command argument length is right
0001c8   50ee     movf	0xee, 0, 0              movf        POSTINC0,w              ; command, just move the pointer
0001ca   0e02     movlw	0x2                    movlw       0x02                    ; correct number of bytes
0001cc   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP               ; put it in temp for error report
0001ce   18ee     xorwf	0xee, 0, 0             xorwf       POSTINC0,w              ; check that the length is right
0001d0   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z
0001d2   ef57     goto	0x8ae                   goto        serial_error_wrong_parameters   ; long bnz
0001d4   f004
                                           ;    bnz         serial_error_wrong_parameters   ; not zero so not equal, report
                                               ; got here so length is good
                                           
                                               ; make sure we can do Z80 bus operations
0001d6   eca9     call	0x952, 0                call        ensure_master
0001d8   f004
                                           
0001da   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0001dc   6f8a     movwf	0x8a, 0x1              movwf       HI_ADDR
0001de   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0001e0   6f89     movwf	0x89, 0x1              movwf       LO_ADDR
                                               ; do the IO read
0001e2   ecef     call	0x9de, 0                call        io_read
0001e4   f004
                                           
                                               ; now return to previous master/slave mode
0001e6   ecb1     call	0x962, 0                call        revert_master
0001e8   f004
                                           
                                               ; now build a serial packet and return it to the host
0001ea   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                               ; reply with the command
0001ec   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
0001ee   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0001f0   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                               ; now the length (1 byte)
0001f2   0e01     movlw	0x1                    movlw       0x01
0001f4   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0001f6   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now the actual data
0001f8   518b     movf	0x8b, 0, 0x1            movf        DREG,w
0001fa   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0001fc   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; finally add the checksum to the packet
0001fe   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
000200   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
000202   ec3a     call	0x74, 0                 call        serial_tx_send_packet
000204   f000
                                           
000206   efa5     goto	0x94a                   goto        serial_handler_exit
000208   f004
                                           
                                           serial_write_io
                                               ; write a single IO port from the serial host
                                               ; Address   2 bytes be
                                               ; Data      1 byte
                                               ; check that the command argument length is right
00020a   50ee     movf	0xee, 0, 0              movf        POSTINC0,w              ; command, just move the pointer
00020c   0e03     movlw	0x3                    movlw       0x03                    ; correct number of bytes
00020e   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP               ; put it in temp for error report
000210   18ee     xorwf	0xee, 0, 0             xorwf       POSTINC0,w              ; check that the length is right
000212   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z                ; long bnz
000214   ef57     goto	0x8ae                   goto        serial_error_wrong_parameters
