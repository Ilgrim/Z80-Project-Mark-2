00021c   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
00021e   6f8a     movwf	0x8a, 0x1              movwf       HI_ADDR
000220   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000222   6f89     movwf	0x89, 0x1              movwf       LO_ADDR
000224   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000226   6f8b     movwf	0x8b, 0x1              movwf       DREG
                                               ; do the IO write
000228   ec00     call	0xa00, 0                call        io_write
00022a   f005
                                           
                                               ; now return to previous master/slave mode
00022c   ecb1     call	0x962, 0                call        revert_master
00022e   f004
                                           
                                               ; now build a serial packet and return it to the host (just an ack)
000230   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                               ; reply with the command
000232   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
000234   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000236   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                               ; now the length (0 bytes)
000238   0e00     movlw	0                      movlw       0x00
00023a   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00023c   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; finally add the checksum to the packet
00023e   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
000240   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
000242   ec3a     call	0x74, 0                 call        serial_tx_send_packet
000244   f000
                                           
000246   efa5     goto	0x94a                   goto        serial_handler_exit
000248   f004
                                           
                                           serial_read_mem
                                               ; read a single byte from system memory
                                               ; Address   2 byte be
                                           
00024a   50ee     movf	0xee, 0, 0              movf        POSTINC0,w      ; dump the command byte
00024c   0e02     movlw	0x2                    movlw       0x02            ; correct number of bytes
00024e   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP       ; put it in temp for error report
000250   18ee     xorwf	0xee, 0, 0             xorwf       POSTINC0,w      ; check the byte length of the command
000252   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z        ; long bnz
000254   ef57     goto	0x8ae                   goto        serial_error_wrong_parameters
000256   f004
                                           
                                               ; make sure we can do Z80 bus operations
000258   eca9     call	0x952, 0                call        ensure_master
00025a   f004
                                           
00025c   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
00025e   6f8a     movwf	0x8a, 0x1              movwf       HI_ADDR
000260   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000262   6f89     movwf	0x89, 0x1              movwf       LO_ADDR
                                           
000264   ec12     call	0xa24, 0                call        mem_read
000266   f005
                                           
000268   ecb1     call	0x962, 0                call        revert_master
00026a   f004
                                           
                                               ; reset the send buffer pointer
00026c   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                               ; reply with the command
00026e   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
000270   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000272   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                               ; now the packet length (1 byte of data)
000274   0e01     movlw	0x1                    movlw       0x01
000276   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000278   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now the data byte
00027a   518b     movf	0x8b, 0, 0x1            movf        DREG,w
00027c   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00027e   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; finally the checksum
000280   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
000282   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                               
000284   ec3a     call	0x74, 0                 call        serial_tx_send_packet
000286   f000
                                           
000288   efa5     goto	0x94a                   goto        serial_handler_exit
00028a   f004
                                           
                                           serial_write_mem
                                               ; write a single byte to system memory
                                               ; Address   2 byte be
                                               ; Data      1 byte
                                           
00028c   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
00028e   0e03     movlw	0x3                    movlw       0x03            ; correct number of bytes
000290   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP
000292   18ee     xorwf	0xee, 0, 0             xorwf       POSTINC0,w
000294   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z
000296   ef57     goto	0x8ae                   goto        serial_error_wrong_parameters
000298   f004
                                           
                                               ; make sure we can do Z80 bus operations
00029a   eca9     call	0x952, 0                call        ensure_master
00029c   f004
                                           
00029e   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0002a0   6f8a     movwf	0x8a, 0x1              movwf       HI_ADDR
0002a2   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0002a4   6f89     movwf	0x89, 0x1              movwf       LO_ADDR
0002a6   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0002a8   6f8b     movwf	0x8b, 0x1              movwf       DREG
                                           
0002aa   ec21     call	0xa42, 0                call        mem_write
0002ac   f005
                                           
0002ae   ecb1     call	0x962, 0                call        revert_master
0002b0   f004
                                           
0002b2   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                           
0002b4   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
0002b6   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0002b8   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                               ; now the packet length (0 bytes, this is just an ACK)
0002ba   0e00     movlw	0                      movlw       0x00
0002bc   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0002be   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; finally add the checksum
0002c0   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
0002c2   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
0002c4   ec3a     call	0x74, 0                 call        serial_tx_send_packet
0002c6   f000
                                           
0002c8   efa5     goto	0x94a                   goto        serial_handler_exit
0002ca   f004
                                           
                                           serial_block_mem_read
                                               ; read a block of memory addresses
                                               ; Start Address     2 byte be
                                               ; Data Count        1 byte
                                           
0002cc   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0002ce   0e03     movlw	0x3                    movlw       0x03
0002d0   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP
0002d2   18ee     xorwf	0xee, 0, 0             xorwf       POSTINC0,w
0002d4   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z
0002d6   ef57     goto	0x8ae                   goto        serial_error_wrong_parameters
0002d8   f004
                                           
                                               ; check that we can do Z80 bus operations
0002da   eca9     call	0x952, 0                call        ensure_master
0002dc   f004
                                           
0002de   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0002e0   6f8a     movwf	0x8a, 0x1              movwf       HI_ADDR
0002e2   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0002e4   6f89     movwf	0x89, 0x1              movwf       LO_ADDR
0002e6   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0002e8   6f86     movwf	0x86, 0x1              movwf       TX_COUNT
                                           
                                               ; ought to check TX_COUNT length here
0002ea   0efe     movlw	0xfe                   movlw       0xFE
0002ec   6186     cpfslt	0x86, 0x1             cpfslt      TX_COUNT
0002ee   ef86     goto	0x90c                   goto        serial_error_packet_length
0002f0   f004
                                           
0002f2   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                           
0002f4   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
0002f6   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0002f8   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
0002fa   5186     movf	0x86, 0, 0x1            movf        TX_COUNT,w
0002fc   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0002fe   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
                                               ; loop call mem_read and store in TX buffer
                                           serial_block_mem_read_loop
000300   ec12     call	0xa24, 0                call        mem_read
000302   f005
000304   518b     movf	0x8b, 0, 0x1            movf        DREG,w
000306   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000308   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
00030a   4b89     infsnz	0x89, 0x1, 0x1        infsnz      LO_ADDR,f
00030c   2b8a     incf	0x8a, 0x1, 0x1          incf        HI_ADDR,f
                                           
00030e   2f86     decfsz	0x86, 0x1, 0x1        decfsz      TX_COUNT,f
000310   d7f7     bra	0x300                    bra         serial_block_mem_read_loop
                                           
                                               ; release the bus
000312   ecb1     call	0x962, 0                call        revert_master
000314   f004
                                           
                                               ; add the checksum
000316   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
000318   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
00031a   ec3a     call	0x74, 0                 call        serial_tx_send_packet
00031c   f000
                                           
00031e   efa5     goto	0x94a                   goto        serial_handler_exit
000320   f004
                                           
                                           serial_block_mem_write
                                               ; copy a block of data from the PC into the memory
                                               ; Start Address:    2 bytes be
                                               ; Data:             Any length 1 or longer
000322   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000324   0e03     movlw	0x3                    movlw       0x03            ; minimum number of parameters
000326   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP
000328   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
00032a   6f86     movwf	0x86, 0x1              movwf       TX_COUNT        ; need to keep a track of this for byte counting
00032c   0e02     movlw	0x2                    movlw       0x02            ; we're going to do a greater than, so needs to be 1 less than min
                                               ; second parameter can be any length of bytes, so the correct length is a minimum
00032e   6586     cpfsgt	0x86, 0x1             cpfsgt      TX_COUNT
000330   ef57     goto	0x8ae                   goto        serial_error_wrong_parameters
000332   f004
                                           
000334   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000336   6f8a     movwf	0x8a, 0x1              movwf       HI_ADDR
000338   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
00033a   6f89     movwf	0x89, 0x1              movwf       LO_ADDR
                                           
                                               ; now set up the byte counter, length of packet data - address field
00033c   0e02     movlw	0x2                    movlw       0x02
00033e   5f86     subwf	0x86, 0x1, 0x1         subwf       TX_COUNT,f      ; take off the address length
                                           
000340   eca9     call	0x952, 0                call        ensure_master
000342   f004
                                           
                                           serial_block_mem_write_loop    
000344   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000346   6f8b     movwf	0x8b, 0x1              movwf       DREG
                                           
000348   ec21     call	0xa42, 0                call        mem_write
00034a   f005
                                           
00034c   4b89     infsnz	0x89, 0x1, 0x1        infsnz      LO_ADDR,f
00034e   2b8a     incf	0x8a, 0x1, 0x1          incf        HI_ADDR,f
                                           
000350   2f86     decfsz	0x86, 0x1, 0x1        decfsz      TX_COUNT,f
000352   d7f8     bra	0x344                    bra         serial_block_mem_write_loop
                                           
000354   ecb1     call	0x962, 0                call        revert_master
000356   f004
                                           
000358   6ae1     clrf	0xe1, 0                 clrf        FSR1L
00035a   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
00035c   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00035e   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                           
000360   0e00     movlw	0                      movlw       0x00
000362   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000364   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
000366   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
000368   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
00036a   ec3a     call	0x74, 0                 call        serial_tx_send_packet
00036c   f000
                                           
00036e   efa5     goto	0x94a                   goto        serial_handler_exit
000370   f004
                                           
                                           serial_bios_update
                                               ; update a 128 byte block of the BIOS image in Flash
                                               ; Start Address:    2 bytes be
                                               ; Data:             128 bytes
000372   50ee     movf	0xee, 0, 0              movf        POSTINC0,w      ; command, dump this
000374   0e82     movlw	0x82                   movlw       0x82            ; 130 exact number of bytes for this packet
000376   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP       ; store the number for a possible error report
000378   18ee     xorwf	0xee, 0, 0             xorwf       POSTINC0,w      ; xor with the length from the packet
00037a   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z        ; if it was zero all ok
00037c   ef57     goto	0x8ae                   goto        serial_error_wrong_parameters   ; if not send an error
00037e   f004
                                           
                                               ; need to check the address is a 128 byte aligned one
                                               ; next byte is the high address byte, can be anything up to 0x1F
000380   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000382   0be0     andlw	0xe0                   andlw       0xE0
000384   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z
000386   ef8a     goto	0x914                   goto        serial_error_bios_address_space
000388   f004
                                           
00038a   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
00038c   0b7f     andlw	0x7f                   andlw       0x7F            ; needs to be 128 bit aligned, so nothing in
                                                                           ; bottom 7 bits to be valid
00038e   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z
000390   ef8e     goto	0x91c                   goto        serial_error_bios_address_offset
000392   f004
                                           
                                               ; looks like there's enough data and the parameters are valid.
                                               ; apply the 24K offset to point the address to the top of ROM
                                           
000394   0e02     movlw	0x2                    movlw       0x02
000396   6ee9     movwf	0xe9, 0                movwf       FSR0L
000398   0e60     movlw	0x60                   movlw       0x60
00039a   26ef     addwf	0xef, 0x1, 0           addwf       INDF0,f         ; add the offset but leave the pointer there
                                           
                                               ; call boot_update to deal with the memory writing
00039c   ec54     call	0xaa8, 0                call        boot_update
00039e   f005
                                           
                                               ; boot_update has built the response packet for us, so just send it
0003a0   ec3a     call	0x74, 0                 call        serial_tx_send_packet
0003a2   f000
                                           
0003a4   efa5     goto	0x94a                   goto        serial_handler_exit
0003a6   f004
                                           
                                           serial_do_commands
                                               ; run a no data command
0003a8   50ee     movf	0xee, 0, 0              movf        POSTINC0,w      ; command 
0003aa   0e01     movlw	0x1                    movlw       0x01
0003ac   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP
0003ae   18ee     xorwf	0xee, 0, 0             xorwf       POSTINC0,w
0003b0   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z
0003b2   ef57     goto	0x8ae                   goto        serial_error_wrong_parameters
0003b4   f004
                                           
                                               ; now perform the lookup
0003b6   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
0003b8   6f87     movwf	0x87, 0x1              movwf       JUMP_REG        ; copy to a holding register
0003ba   6b88     clrf	0x88, 0x1               clrf        MAIN_TEMP       ; clear for high byte of address jump
0003bc   90d8     bcf	0xd8, 0, 0               bcf         STATUS,C        ; clear the carry bit to rotate into bit 0
0003be   3787     rlcf	0x87, 0x1, 0x1          rlcf        JUMP_REG,f      ; rotate storing MSB in C == x 2
0003c0   b0d8     btfsc	0xd8, 0, 0             btfsc       STATUS,C        ; see if MSB was set
0003c2   8388     bsf	0x88, 0x1, 0x1           bsf         MAIN_TEMP,1     ; if it was add 2 to high reg (gonna x2 later)
0003c4   90d8     bcf	0xd8, 0, 0               bcf         STATUS,C        ; clear the carry bit again
0003c6   3787     rlcf	0x87, 0x1, 0x1          rlcf        JUMP_REG,f      ; rotate again x4 now
0003c8   b0d8     btfsc	0xd8, 0, 0             btfsc       STATUS,C        ; see if high bit was set
0003ca   8188     bsf	0x88, 0, 0x1             bsf         MAIN_TEMP,0     ; add 1 to the high byte register
                                           
0003cc   0ede     movlw	0xde                   movlw       LOW serial_cmd_jumps    ; copy the low address for jump table
0003ce   2787     addwf	0x87, 0x1, 0x1         addwf       JUMP_REG,f      ; add it to the jump address
0003d0   b0d8     btfsc	0xd8, 0, 0             btfsc       STATUS,C        ; see if the add overflowed
0003d2   2b88     incf	0x88, 0x1, 0x1          incf        MAIN_TEMP,f     ; if so increment the high address
0003d4   0e03     movlw	0x3                    movlw       HIGH serial_cmd_jumps   ; get the high address byte
0003d6   2588     addwf	0x88, 0, 0x1           addwf       MAIN_TEMP,w     ; add it to the high address
                                           
0003d8   6efa     movwf	0xfa, 0                movwf       PCLATH          ; put the high byte into the PCLATH
0003da   5187     movf	0x87, 0, 0x1            movf        JUMP_REG,w
0003dc   6ef9     movwf	0xf9, 0                movwf       PCL             ; put the low address into the PC
                                           
                                           serial_cmd_jumps
                                               ; now put 256 goto commands
0003de   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 0                     
0003e0   f004
0003e2   efef     goto	0x7de                   goto        serial_do_get_reset             ; 1                     
0003e4   f003
0003e6   eff5     goto	0x7ea                   goto        serial_do_get_dma               ; 2                     
0003e8   f003
0003ea   effb     goto	0x7f6                   goto        serial_do_get_slave             ; 3                     
0003ec   f003
0003ee   ef01     goto	0x802                   goto        serial_do_reset                 ; 4                     
0003f0   f004
0003f2   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 5                     
0003f4   f004
0003f6   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 6                     
0003f8   f004
0003fa   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 7                     
0003fc   f004
