                                           list p=18f4520
                                           #include <p18f4520.inc>
                                                   LIST
                                           
                                           ;==========================================================================
                                           ;  MPASM PIC18F4520 processor include
                                           ; 
                                           ;  (c) Copyright 1999-2007 Microchip Technology, All rights reserved
                                           ;==========================================================================
                                           
                                                   NOLIST
                                           #include "portpins.inc"
                                           ;== PORT Definitions ==========================================================
                                           
                                           ;-- PORT A --------------------------------------------------------------------
                                           
                                           P_RESET         EQU     0
                                           P_BUSRQ         EQU     1
                                           P_MREQ          EQU     2
                                           P_IORQ          EQU     3
                                           P_WAIT          EQU     4
                                           P_HI_LAT        EQU     5
                                           
                                           ;-- PORT C --------------------------------------------------------------------
                                           
                                           P_BUSACK        EQU     0
                                           P_SD_CS         EQU     1
                                           P_CLK           EQU     2
                                           P_SD_CK         EQU     3
                                           P_SD_DI         EQU     4
                                           P_SD_DO         EQU     5
                                           P_TX            EQU     6
                                           P_RX            EQU     7
                                           
                                           ;-- PORT E --------------------------------------------------------------------
                                           
                                           P_RD            EQU     0
                                           P_WR            EQU     1
                                           P_CS            EQU     2
                                           
                                           ; -- Externals From host_bus.asm ----------------------------------------------
                                           
                                               EXTERN      ensure_master
                                               EXTERN      revert_master
                                               EXTERN      mem_write
                                           
                                               EXTERN      DREG
                                               EXTERN      HI_ADDR
                                               EXTERN      LO_ADDR
                                           
                                             UDATA
                                           
                                           WRCOUNT         RES     1
                                           BOOTWR_FLAGS      RES     1
                                           
                                             CODE
                                           
                                           boot_init
                                           
                                           ; set up the PWM peripheral that's going to generate the clock
000a64   0e27     movlw	0x27                   movlw       d'39'            ; Frequency setting  1 = 5MHz, 2 = 3.33MHz, 
                                                                            ;                    3 = 2.5MHz, 39 = 250kHz
000a66   6ecb     movwf	0xcb, 0                movwf       PR2
                                           ; Duty cycle 1/2 
                                           ; 10 bit value bits <9:2> in CCPR1L
                                           ;              bits <1:0> in CCP1CON<5:4>
                                           ; 5MHz = 4    (b'0000000100' - 0x01 & b'00')
                                           ; 3.33MHz = 6 (b'0000000110' - 0x01 & b'10')
                                           ; 2.5MHz = 16 (b'0000010000' - 0x02 & b'00')
                                           ; 250kHz = 80 (b'0001010000' - 0x14 & b'00')
                                           ; CCP1CON<7:6> 00 - single output
                                           ; CCP1CON<5:4> xx - see above
                                           ; CCP1CON<3:0> 1100 - PWM mode
000a68   0e0c     movlw	0xc                    movlw       b'00001100'
000a6a   6ebd     movwf	0xbd, 0                movwf       CCP1CON
000a6c   0e14     movlw	0x14                   movlw       0x14
000a6e   6ebe     movwf	0xbe, 0                movwf       CCPR1L          
                                           
000a70   9494     bcf	0x94, 0x2, 0             bcf         TRISC,P_CLK     ; pin as output
                                               
000a72   0e04     movlw	0x4                    movlw       b'00000100'     ; T2 on, no pre or post scale
000a74   6eca     movwf	0xca, 0                movwf       T2CON
                                           
000a76   0012     return	0                     return
                                           
                                           ;*******************************************************************************
                                           ;*  boot_load - loads the boot ROM from the PIC's flash to system memory       *
                                           ;*******************************************************************************
                                           boot_load
                                               ; make sure that the Z80 is a slave
000a78   eca9     call	0x952, 0                call	ensure_master
000a7a   f004
                                           
                                               ; set up the table read pointers
                                           
000a7c   6af8     clrf	0xf8, 0                 clrf        TBLPTRU
000a7e   0e60     movlw	0x60                   movlw       0x60
000a80   6ef7     movwf	0xf7, 0                movwf       TBLPTRH
000a82   6af6     clrf	0xf6, 0                 clrf        TBLPTRL
                                           
                                               ; set up EECON1 for Flash memory read
                                               
000a84   0e80     movlw	0x80                   movlw       b'10000000'
000a86   6ea6     movwf	0xa6, 0                movwf       EECON1
                                           
                                               ; now loop over all 8KB of data
000a88   6b8a     clrf	0x8a, 0x1               clrf        HI_ADDR
000a8a   6b89     clrf	0x89, 0x1               clrf        LO_ADDR
                                           
                                           boot_load_loop
000a8c   0009     tblrd	*+                     tblrd*+             ; copy a byte into TABLAT
000a8e   cff5     movff	0xff5, 0x8b            movff       TABLAT,DREG
000a90   f08b
000a92   ec21     call	0xa42, 0                call        mem_write
000a94   f005
000a96   3f89     incfsz	0x89, 0x1, 0x1        incfsz      LO_ADDR,f
000a98   d7f9     bra	0xa8c                    bra         boot_load_loop
000a9a   2b8a     incf	0x8a, 0x1, 0x1          incf        HI_ADDR,f
000a9c   518a     movf	0x8a, 0, 0x1            movf        HI_ADDR,w
000a9e   0a20     xorlw	0x20                   xorlw       0x20
000aa0   e1f5     bnz	0xa8c                    bnz         boot_load_loop
                                           
                                               ; all done, release the bus if we claimed it and return
000aa2   ecb1     call	0x962, 0                call        revert_master
000aa4   f004
                                           
000aa6   0012     return	0                     return
                                           
                                           
                                           ;*******************************************************************************
                                           ;*  boot_update - copy a 128 byte packet from RX buffer to flash               *
                                           ;*    This is called repeatedly from serial.asm after an update BIOS command   *
                                           ;*    is sent from a PC.                                                       *
                                           ;*******************************************************************************
                                           boot_update
                                               ; first two bytes in the RX buffer are the start address in FLASH
                                               ; they've already been checked so just trust them
                                               ; FSR0L is already pointing at the high address
                                           
000aa8   6af8     clrf	0xf8, 0                 clrf        TBLPTRU
000aaa   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000aac   6ef7     movwf	0xf7, 0                movwf       TBLPTRH
000aae   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000ab0   6ef6     movwf	0xf6, 0                movwf       TBLPTRL
                                           
000ab2   818f     bsf	0x8f, 0, 0x1             bsf         BOOTWR_FLAGS,0
                                               ; next code gets run twice because you can only erase data in 64 byte blocks
                                           boot_update_erase
                                               ; erase a block
000ab4   8ea6     bsf	0xa6, 0x7, 0             bsf         EECON1, EEPGD
000ab6   9ca6     bcf	0xa6, 0x6, 0             bcf         EECON1, CFGS
000ab8   84a6     bsf	0xa6, 0x2, 0             bsf         EECON1, WREN
000aba   88a6     bsf	0xa6, 0x4, 0             bsf         EECON1, FREE
000abc   9ef2     bcf	0xf2, 0x7, 0             bcf         INTCON, GIE
                                           
000abe   0e55     movlw	0x55                   movlw       0x55
000ac0   6ea7     movwf	0xa7, 0                movwf       EECON2
000ac2   0eaa     movlw	0xaa                   movlw       0x0AA
000ac4   6ea7     movwf	0xa7, 0                movwf       EECON2
000ac6   82a6     bsf	0xa6, 0x1, 0             bsf         EECON1, WR              ; actual erase command up to 2 ms
                                           
000ac8   8ef2     bsf	0xf2, 0x7, 0             bsf         INTCON, GIE     ; re-enable interrupts
000aca   000a     tblrd	*-                     tblrd*-                     ; dummy decrement, we're going to use pre-inc
                                                                           ; to write so the pointer is still in the
                                                                           ; block when we call write
                                           
000acc   838f     bsf	0x8f, 0x1, 0x1           bsf         BOOTWR_FLAGS,1  ; flag to see how many times we've done this
                                               ; write a 32 byte block, happens twice
                                           boot_update_write
000ace   0e20     movlw	0x20                   movlw       d'32'
000ad0   6f8e     movwf	0x8e, 0x1              movwf       WRCOUNT
                                           
                                           boot_update_write_loop
000ad2   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000ad4   6ef5     movwf	0xf5, 0                movwf       TABLAT
000ad6   000f     tblwt	+*                     tblwt+*
000ad8   2f8e     decfsz	0x8e, 0x1, 0x1        decfsz      WRCOUNT,f
000ada   d7fb     bra	0xad2                    bra         boot_update_write_loop
                                           
                                               ; written 32 bytes to tablat, now need to initiate the write
000adc   8ea6     bsf	0xa6, 0x7, 0             bsf         EECON1, EEPGD
000ade   9ca6     bcf	0xa6, 0x6, 0             bcf         EECON1, CFGS
000ae0   84a6     bsf	0xa6, 0x2, 0             bsf         EECON1, WREN
000ae2   9ef2     bcf	0xf2, 0x7, 0             bcf         INTCON, GIE
                                           
000ae4   0e55     movlw	0x55                   movlw       0x55
000ae6   6ea7     movwf	0xa7, 0                movwf       EECON2
000ae8   0eaa     movlw	0xaa                   movlw       0x0AA
000aea   6ea7     movwf	0xa7, 0                movwf       EECON2
000aec   82a6     bsf	0xa6, 0x1, 0             bsf         EECON1, WR
                                           
000aee   8ef2     bsf	0xf2, 0x7, 0             bsf         INTCON, GIE
000af0   94a6     bcf	0xa6, 0x2, 0             bcf         EECON1, WREN
                                           
000af2   a38f     btfss	0x8f, 0x1, 0x1         btfss       BOOTWR_FLAGS,1
000af4   d002     bra	0xafa                    bra         boot_update_second_erase
                                           
000af6   938f     bcf	0x8f, 0x1, 0x1           bcf         BOOTWR_FLAGS,1
000af8   d7ea     bra	0xace                    bra         boot_update_write
                                           
                                           boot_update_second_erase
000afa   a18f     btfss	0x8f, 0, 0x1           btfss       BOOTWR_FLAGS,0
000afc   ef84     goto	0xb08                   goto        boot_verify
000afe   f005
000b00   918f     bcf	0x8f, 0, 0x1             bcf         BOOTWR_FLAGS,0
000b02   0009     tblrd	*+                     tblrd*+                     ; dummy increment to make sure we erase the next block
000b04   ef5a     goto	0xab4                   goto        boot_update_erase
000b06   f005
                                           
                                           boot_verify
                                               ; check through all the bytes just written to flash and make sure they
                                               ; match.  Then build a report packet accordingly
000b08   858f     bsf	0x8f, 0x2, 0x1           bsf         BOOTWR_FLAGS,2  ; set a flag, this gets cleared if we find an
                                                                           ; error
                                           
000b0a   0e01     movlw	0x1                    movlw       0x1
000b0c   6eea     movwf	0xea, 0                movwf       FSR0H
000b0e   0e02     movlw	0x2                    movlw       0x02            ; need to skip the command and length bytes
000b10   6ee9     movwf	0xe9, 0                movwf       FSR0L
                                           
                                               ; set the table pointer
000b12   6af8     clrf	0xf8, 0                 clrf        TBLPTRU
000b14   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000b16   6ef7     movwf	0xf7, 0                movwf       TBLPTRH
000b18   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000b1a   6ef6     movwf	0xf6, 0                movwf       TBLPTRL
                                           
000b1c   0e80     movlw	0x80                   movlw       0x80
000b1e   6f8e     movwf	0x8e, 0x1              movwf       WRCOUNT
                                           
                                           boot_verify_loop
000b20   50ee     movf	0xee, 0, 0              movf        POSTINC0,w
000b22   0009     tblrd	*+                     tblrd*+
000b24   18f5     xorwf	0xf5, 0, 0             xorwf       TABLAT,w
000b26   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z
000b28   958f     bcf	0x8f, 0x2, 0x1           bcf         BOOTWR_FLAGS,2
000b2a   2f8e     decfsz	0x8e, 0x1, 0x1        decfsz      WRCOUNT,f
000b2c   d7f9     bra	0xb20                    bra         boot_verify_loop
                                           
                                               ; all verified, need to make a response packet based on the OK flag
000b2e   a58f     btfss	0x8f, 0x2, 0x1         btfss       BOOTWR_FLAGS,2
000b30   d00a     bra	0xb46                    bra         boot_verify_nak
                                           
                                               ; got here so write went according to plan
000b32   0e02     movlw	0x2                    movlw       0x2
000b34   6ee2     movwf	0xe2, 0                movwf       FSR1H
000b36   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                           
000b38   0e08     movlw	0x8                    movlw       0x08            ; successful command 8 executed
000b3a   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000b3c   0e00     movlw	0                      movlw       0x00            ; no data, just ack
000b3e   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000b40   0e08     movlw	0x8                    movlw       0x08            ; hard coded checksum Oo
000b42   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
000b44   0012     return	0                     return
                                           
                                           boot_verify_nak
                                               ; there must have been an error in the memory writing, send a NAK
000b46   0e02     movlw	0x2                    movlw       0x02
000b48   6ee2     movwf	0xe2, 0                movwf       FSR1H
000b4a   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                           
                                               ; going to cheat and use the WRCOUNT as a checksum holder
000b4c   6b8e     clrf	0x8e, 0x1               clrf        WRCOUNT
                                           
000b4e   0e48     movlw	0x48                   movlw       0x48            ; nak for command 8
000b50   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000b52   1b8e     xorwf	0x8e, 0x1, 0x1         xorwf       WRCOUNT,f
000b54   0e02     movlw	0x2                    movlw       0x02            ; only the error code to reply with
000b56   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000b58   1b8e     xorwf	0x8e, 0x1, 0x1         xorwf       WRCOUNT,f
000b5a   0e00     movlw	0                      movlw       0x00
000b5c   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000b5e   1b8e     xorwf	0x8e, 0x1, 0x1         xorwf       WRCOUNT,f
000b60   0e08     movlw	0x8                    movlw       0x08            ; verification error number
000b62   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000b64   1b8e     xorwf	0x8e, 0x1, 0x1         xorwf       WRCOUNT,f
000b66   518e     movf	0x8e, 0, 0x1            movf        WRCOUNT,w
000b68   6ee6     movwf	0xe6, 0                movwf       POSTINC1        ; add the checksum
                                           
000b6a   0012     return	0                     return
                                               
                                           
                                           ; == Export These ==============================================================
                                           
                                               GLOBAL      boot_init
                                               GLOBAL      boot_load
                                               GLOBAL      boot_update
                                           
                                           end
                                           
