                                           list p=18f4520
                                           #include <p18f4520.inc>
                                                   LIST
                                           
                                           ;==========================================================================
                                           ;  MPASM PIC18F4520 processor include
                                           ; 
                                           ;  (c) Copyright 1999-2007 Microchip Technology, All rights reserved
                                           ;==========================================================================
                                           
                                                   NOLIST
                                           #include "portpins.inc"
                                           ;== PORT Definitions ==========================================================
                                           
                                           ;-- PORT A --------------------------------------------------------------------
                                           
                                           P_RESET         EQU     0
                                           P_BUSRQ         EQU     1
                                           P_MREQ          EQU     2
                                           P_IORQ          EQU     3
                                           P_WAIT          EQU     4
                                           P_HI_LAT        EQU     5
                                           
                                           ;-- PORT C --------------------------------------------------------------------
                                           
                                           P_BUSACK        EQU     0
                                           P_SD_CS         EQU     1
                                           P_CLK           EQU     2
                                           P_SD_CK         EQU     3
                                           P_SD_DI         EQU     4
                                           P_SD_DO         EQU     5
                                           P_TX            EQU     6
                                           P_RX            EQU     7
                                           
                                           ;-- PORT E --------------------------------------------------------------------
                                           
                                           P_RD            EQU     0
                                           P_WR            EQU     1
                                           P_CS            EQU     2
                                           
                                           errorlevel -302
                                           errorlevel -205
                                           
                                           CONFIG OSC = HSPLL
                                           CONFIG FCMEN = ON, IESO = OFF, PWRT = ON, BOREN = OFF, BORV = 0
                                           CONFIG WDT = OFF, WDTPS = 1, MCLRE = ON, LPT1OSC = OFF
                                           CONFIG PBADEN = OFF, CCP2MX = PORTC, STVREN = ON, LVP = OFF
                                           CONFIG XINST = OFF, DEBUG = OFF, CP0 = OFF, CP1 = OFF, CP2 = OFF, CP3 = OFF
                                           CONFIG CPB = OFF, CPD = OFF, WRT0 = OFF, WRT1 = OFF, WRT2 = OFF, WRT3 = OFF
                                           CONFIG WRTB = OFF, WRTC = OFF, WRTD = OFF, EBTR0 = OFF, EBTR1 = OFF
                                           CONFIG EBTR2 = OFF, EBTR3 = OFF, EBTRB = OFF
                                           
                                           ; -- Externals From serial.asm ------------------------------------------------
                                           
                                               EXTERN      serial_rx_int
                                               EXTERN      serial_init
                                               EXTERN      serial_command_dispatch
                                           
                                           ; -- Externals From host_bus.asm ----------------------------------------------
                                           
                                               EXTERN      get_reset
                                               EXTERN      get_dma
                                               EXTERN      get_slave
                                               EXTERN      ensure_master
                                               EXTERN      revert_master
                                           
                                           ; -- Externals From boot.asm ---------------------------------------------------
                                           
                                               EXTERN      boot_init
                                               EXTERN      boot_load
                                           
                                           ;== Variables =================================================================
                                           
                                           UDATA
                                           
                                           MAIN_TEMP       RES     1
                                           
                                           org 0x00
000000   ef08     goto	0x10                    goto init
000002   f000
                                           
                                           org 0x08
                                           interrupt
000008   ba9e     btfsc	0x9e, 0x5, 0           btfsc       PIR1,RCIF       ;test serial receive interrupt flag
00000a   ec55     call	0xaa, 0                 call        serial_rx_int   ;set, so handle received data
00000c   f000
00000e   0011     retfie	0x1                   retfie      FAST            ;all interrupts serviced, return reinstating context
                                           
                                           init
000010   0e0f     movlw	0xf                    movlw       0x0F
000012   6ec1     movwf	0xc1, 0                movwf       ADCON1          ; no ADC functionality thanks.
000014   eccc     call	0x998, 0                call        get_reset       ; put the Z80 into reset before we start doing stuff
000016   f004
000018   ec1b     call	0x36, 0                 call        serial_init     ; initialise the serial port
00001a   f000
00001c   ec32     call	0xa64, 0                call        boot_init       ; set up CPU clock etc.
00001e   f005
                                           
000020   9a94     bcf	0x94, 0x5, 0             bcf         TRISC,5         ; the extra LED on SD card port needs output
000022   9a8b     bcf	0x8b, 0x5, 0             bcf         LATC,5
                                           
000024   ec3c     call	0xa78, 0                call        boot_load
000026   f005
                                               
000028   8a8b     bsf	0x8b, 0x5, 0             bsf         LATC,5
                                           
                                               ; let the Z80 go!!
00002a   ecda     call	0x9b4, 0                call        get_slave
00002c   f004
                                           
                                           main
                                               ; call serial_command_dispatch, checks for a whole command received and
                                               ; calls an appropriate handler function for the command
00002e   ec90     call	0x120, 0                call        serial_command_dispatch
000030   f000
000032   ef17     goto	0x2e                    goto        main
000034   f000
                                           
                                           ; == Export these =============================================================
                                           
                                               GLOBAL      MAIN_TEMP
                                           end
                                           ;==============================================================================
                                           ;    host_bus.asm
                                           ;==============================================================================
                                           list p=18f4520
                                           #include <p18f4520.inc>
                                                   LIST
                                           
                                           ;==========================================================================
                                           ;  MPASM PIC18F4520 processor include
                                           ; 
                                           ;  (c) Copyright 1999-2007 Microchip Technology, All rights reserved
                                           ;==========================================================================
                                           
                                                   NOLIST
                                           #include "portpins.inc"
                                           ;== PORT Definitions ==========================================================
                                           
                                           ;-- PORT A --------------------------------------------------------------------
                                           
                                           P_RESET         EQU     0
                                           P_BUSRQ         EQU     1
                                           P_MREQ          EQU     2
                                           P_IORQ          EQU     3
                                           P_WAIT          EQU     4
                                           P_HI_LAT        EQU     5
                                           
                                           ;-- PORT C --------------------------------------------------------------------
                                           
                                           P_BUSACK        EQU     0
                                           P_SD_CS         EQU     1
                                           P_CLK           EQU     2
                                           P_SD_CK         EQU     3
                                           P_SD_DI         EQU     4
                                           P_SD_DO         EQU     5
                                           P_TX            EQU     6
                                           P_RX            EQU     7
                                           
                                           ;-- PORT E --------------------------------------------------------------------
                                           
                                           P_RD            EQU     0
                                           P_WR            EQU     1
                                           P_CS            EQU     2
                                           
                                               UDATA
                                           LO_ADDR RES     1
                                           HI_ADDR RES     1
                                           DREG    RES     1
                                           
                                           MODE            RES     1
                                           TEMP_MODE       RES     1
                                           
                                               CODE
                                           
                                           ;== Z80 Bus interfacing functions =============================================
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* ensure_master - if in slave mode acquire DMA control and return.           *
                                           ;*                 otherwise return immediately.  Note current state.         *
                                           ;*                                                                            *
                                           ;* Inputs:  NONE                                                              *
                                           ;* Outputs: NONE                                                              *
                                           ;* Called:  set_master                                                        *
                                           ;* Changes: TEMP_MODE                                                         *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           ensure_master
                                               ; check_mode
000952   518c     movf	0x8c, 0, 0x1            movf        MODE,w
000954   6f8d     movwf	0x8d, 0x1              movwf       TEMP_MODE       ; save the mode at the entry to this function
000956   0a00     xorlw	0                      xorlw       0x00     
000958   e001     bz	0x95c                     bz          ensure_master_not_master
00095a   0012     return	0                     return                      ; already master so just return
                                           ensure_master_not_master
                                               ; not master need to get DMA mode
00095c   ecc4     call	0x988, 0                call        get_dma
00095e   f004
000960   0012     return	0                     return
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* revert_master - set master mode back to the mode in TEMP_MODE              *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           revert_master
000962   518c     movf	0x8c, 0, 0x1            movf        MODE,w
000964   198d     xorwf	0x8d, 0, 0x1           xorwf       TEMP_MODE,w
000966   e101     bnz	0x96a                    bnz         revert_master_needs_change
000968   0012     return	0                     return              ; MODE and TEMP_MODE matched don't change
                                           revert_master_needs_change
00096a   b18d     btfsc	0x8d, 0, 0x1           btfsc       TEMP_MODE,0     ; test reset mode bit
00096c   efbe     goto	0x97c                   goto        revert_master_reset
00096e   f004
000970   b38d     btfsc	0x8d, 0x1, 0x1         btfsc       TEMP_MODE,1     ; test DMA master mode bit
000972   efc1     goto	0x982                   goto        revert_master_dma
000974   f004
                                               ; if neither of those need to switch back to slave
000976   ecda     call	0x9b4, 0                call        get_slave
000978   f004
00097a   0012     return	0                     return
                                           
                                           revert_master_reset
00097c   eccc     call	0x998, 0                call        get_reset       ; assert the reset line
00097e   f004
000980   0012     return	0                     return
                                           
                                           revert_master_dma
000982   ecc4     call	0x988, 0                call        get_dma         ; get DMA control
000984   f004
000986   0012     return	0                     return
                                           
                                           
                                           get_dma
000988   9289     bcf	0x89, 0x1, 0             bcf         LATA,1          ; assert the BUSRQ signal low
                                           get_dma_loop
00098a   b082     btfsc	0x82, 0, 0             btfsc       PORTC,0         ; wait for BUSACK to go low
00098c   d7fe     bra	0x98a                    bra         get_dma_loop
00098e   0e02     movlw	0x2                    movlw       0x02
000990   6f8c     movwf	0x8c, 0x1              movwf       MODE            ; set mode to DMA master
000992   ecde     call	0x9bc, 0                call        set_master
000994   f004
000996   0012     return	0                     return
                                           
                                           get_reset
000998   9089     bcf	0x89, 0, 0               bcf         LATA,0          ; pull reset low
00099a   0000     nop                          nop
00099c   0000     nop                          nop
00099e   0000     nop                          nop
0009a0   0000     nop                          nop
0009a2   0000     nop                          nop
0009a4   0000     nop                          nop
0009a6   0000     nop                          nop
0009a8   0000     nop                          nop                         ; leave time for the M cycle to finish
0009aa   0e01     movlw	0x1                    movlw       0x01
0009ac   6f8c     movwf	0x8c, 0x1              movwf       MODE
0009ae   ecde     call	0x9bc, 0                call        set_master
0009b0   f004
0009b2   0012     return	0                     return
                                           
                                           get_slave
0009b4   6b8c     clrf	0x8c, 0x1               clrf        MODE            ; set mode to slave
0009b6   ece8     call	0x9d0, 0                call        set_slave       ; set IO into slave mode
0009b8   f004
0009ba   0012     return	0                     return
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* set_master - configure pins so PIC is master device                        *
                                           ;*                                                                            *
                                           ;* Inputs:  NONE                                                              *
                                           ;* Outputs: NONE                                                              *
                                           ;* Called:                                                                    *
                                           ;* Changes: TRISB, TRISE, TRISA                                               *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           set_master
                                           
                                           
                                               ; set the address pins to output
0009bc   6a93     clrf	0x93, 0                 clrf        TRISB
0009be   0eec     movlw	0xec                   movlw       b'11101100'     ;RD and WR as output, disable PSP
0009c0   1696     andwf	0x96, 0x1, 0           andwf       TRISE,f
0009c2   0eff     movlw	0xff                   movlw       0xFF
0009c4   6e8d     movwf	0x8d, 0                movwf       LATE            ;set RD and WR High
                                           
0009c6   0ed0     movlw	0xd0                   movlw       b'11010000'
0009c8   1692     andwf	0x92, 0x1, 0           andwf       TRISA,f
0009ca   0e3c     movlw	0x3c                   movlw       b'00111100'
0009cc   1289     iorwf	0x89, 0x1, 0           iorwf       LATA,f
0009ce   0012     return	0                     return
                                           
                                           set_slave
0009d0   6893     setf	0x93, 0                 setf        TRISB
0009d2   0e17     movlw	0x17                   movlw       b'00010111'     ; PSP mode
0009d4   6e96     movwf	0x96, 0                movwf       TRISE
0009d6   6889     setf	0x89, 0                 setf        LATA            ; all pins in A are high or don't care
0009d8   0ecc     movlw	0xcc                   movlw       b'11001100'     ; MREQ, IORQ input
0009da   6e92     movwf	0x92, 0                movwf       TRISA
0009dc   0012     return	0                     return
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* io_read - Read an address on the IO bus                                    *
                                           ;*                                                                            *
                                           ;* Inputs:  HI_ADDR, LO_ADDR                                                  *
                                           ;* Outputs: DREG                                                              *
                                           ;* Called:                                                                    *
                                           ;* Changes:                                                                   *
                                           ;*                                                                            *
                                           ;* Asssumes the PIC is in master mode (WAIT in, RD,WR,IORQ,MREQ and ADDR out  *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           io_read
                                               ; set address
0009de   518a     movf	0x8a, 0, 0x1            movf    HI_ADDR,w
0009e0   6e8a     movwf	0x8a, 0                movwf   LATB
0009e2   9a89     bcf	0x89, 0x5, 0             bcf     LATA,P_HI_LAT
0009e4   5189     movf	0x89, 0, 0x1            movf    LO_ADDR,w
0009e6   8a89     bsf	0x89, 0x5, 0             bsf     LATA,P_HI_LAT
0009e8   6e8a     movwf	0x8a, 0                movwf   LATB
                                               ; 200ns delay before IORQ and RD
0009ea   0000     nop                          nop
0009ec   9689     bcf	0x89, 0x3, 0             bcf     LATA,P_IORQ
0009ee   908d     bcf	0x8d, 0, 0               bcf     LATE,P_RD
0009f0   0000     nop                          nop
                                               ; mandatory wait state
                                           io_read_wait_loop
0009f2   a880     btfss	0x80, 0x4, 0           btfss   PORTA,P_WAIT
0009f4   d7fe     bra	0x9f2                    bra     io_read_wait_loop
                                               ; store the read data
0009f6   5083     movf	0x83, 0, 0              movf    PORTD,w
                                               ;release IORQ and RD
0009f8   808d     bsf	0x8d, 0, 0               bsf     LATE,P_RD
0009fa   8689     bsf	0x89, 0x3, 0             bsf     LATA,P_IORQ
0009fc   6f8b     movwf	0x8b, 0x1              movwf   DREG
0009fe   0012     return	0                     return
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* io_write - Write an address on the IO bus                                  *
                                           ;*                                                                            *
                                           ;* Inputs:  HI_ADDR, LO_ADDR, DREG                                            *
                                           ;* Outputs: NONE                                                              *
                                           ;* Called:                                                                    *
                                           ;* Changes:                                                                   *
                                           ;*                                                                            *
                                           ;* Asssumes the PIC is in master mode (WAIT in, RD,WR,IORQ,MREQ and ADDR out  *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           io_write
                                               ; set address
000a00   518a     movf	0x8a, 0, 0x1            movf        HI_ADDR,w
000a02   6e8a     movwf	0x8a, 0                movwf       LATB
000a04   9a89     bcf	0x89, 0x5, 0             bcf         LATA,P_HI_LAT
000a06   5189     movf	0x89, 0, 0x1            movf        LO_ADDR,w
000a08   8a89     bsf	0x89, 0x5, 0             bsf         LATA,P_HI_LAT
000a0a   6e8a     movwf	0x8a, 0                movwf       LATB
                                               ; 200ns delay before IORQ and RD
000a0c   518b     movf	0x8b, 0, 0x1            movf        DREG,w          ; write the data to the bus
000a0e   6e8c     movwf	0x8c, 0                movwf       LATD
000a10   6a95     clrf	0x95, 0                 clrf        TRISD           ; don't forget to drive the bus for a write!!
000a12   9689     bcf	0x89, 0x3, 0             bcf         LATA,P_IORQ
000a14   928d     bcf	0x8d, 0x1, 0             bcf         LATE,P_WR
000a16   0000     nop                          nop
                                               ; mandatory wait state
                                           io_write_wait_loop
000a18   a880     btfss	0x80, 0x4, 0           btfss       PORTA,P_WAIT
000a1a   d7fe     bra	0xa18                    bra         io_write_wait_loop
                                               ;release IORQ and RD
000a1c   828d     bsf	0x8d, 0x1, 0             bsf         LATE,P_WR
000a1e   8689     bsf	0x89, 0x3, 0             bsf         LATA,P_IORQ
000a20   6895     setf	0x95, 0                 setf        TRISD           ; stop driving the bus again
000a22   0012     return	0                     return
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* mem_read - Read from an address on the memory bus                          *
                                           ;*                                                                            *
                                           ;* Inputs:  HI_ADDR, LO_ADDR                                                  *
                                           ;* Outputs: DREG                                                              *
                                           ;* Called:                                                                    *
                                           ;* Changes:                                                                   *
                                           ;*                                                                            *
                                           ;* Asssumes the PIC is in master mode (WAIT in, RD,WR,IORQ,MREQ and ADDR out) *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           mem_read
                                               ; set address
000a24   518a     movf	0x8a, 0, 0x1            movf    HI_ADDR,w
000a26   6e8a     movwf	0x8a, 0                movwf   LATB
000a28   9a89     bcf	0x89, 0x5, 0             bcf     LATA,P_HI_LAT
000a2a   5189     movf	0x89, 0, 0x1            movf    LO_ADDR,w
000a2c   8a89     bsf	0x89, 0x5, 0             bsf     LATA,P_HI_LAT
000a2e   6e8a     movwf	0x8a, 0                movwf   LATB
000a30   9489     bcf	0x89, 0x2, 0             bcf     LATA,P_MREQ
000a32   908d     bcf	0x8d, 0, 0               bcf     LATE,P_RD
                                           mem_read_wait_loop
000a34   a880     btfss	0x80, 0x4, 0           btfss   PORTA,P_WAIT
000a36   d7fe     bra	0xa34                    bra     mem_read_wait_loop
                                               ; store the read data
000a38   5083     movf	0x83, 0, 0              movf    PORTD,w
                                               ;release IORQ and RD
000a3a   808d     bsf	0x8d, 0, 0               bsf     LATE,P_RD
000a3c   8489     bsf	0x89, 0x2, 0             bsf     LATA,P_MREQ
000a3e   6f8b     movwf	0x8b, 0x1              movwf   DREG
000a40   0012     return	0                     return
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* mem_write - Write to an address on the memory bus                          *
                                           ;*                                                                            *
                                           ;* Inputs:  HI_ADDR, LO_ADDR, DREG                                            *
                                           ;* Outputs: NONE                                                              *
                                           ;* Called:                                                                    *
                                           ;* Changes:                                                                   *
                                           ;*                                                                            *
                                           ;* Asssumes the PIC is in master mode (WAIT in, RD,WR,IORQ,MREQ and ADDR out) *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           mem_write
                                               ; set address
000a42   518a     movf	0x8a, 0, 0x1            movf        HI_ADDR,w
000a44   6e8a     movwf	0x8a, 0                movwf       LATB
000a46   9a89     bcf	0x89, 0x5, 0             bcf         LATA,P_HI_LAT
000a48   5189     movf	0x89, 0, 0x1            movf        LO_ADDR,w
000a4a   8a89     bsf	0x89, 0x5, 0             bsf         LATA,P_HI_LAT
000a4c   6e8a     movwf	0x8a, 0                movwf       LATB
                                               ; 200ns delay before IORQ and RD
000a4e   518b     movf	0x8b, 0, 0x1            movf        DREG,w          ; write the data to the bus
000a50   6e8c     movwf	0x8c, 0                movwf       LATD
000a52   6a95     clrf	0x95, 0                 clrf        TRISD           ; don't forget to drive the bus for a write!!
000a54   9489     bcf	0x89, 0x2, 0             bcf         LATA,P_MREQ
000a56   928d     bcf	0x8d, 0x1, 0             bcf         LATE,P_WR
                                               ; mandatory wait state
                                           mem_write_wait_loop
000a58   a880     btfss	0x80, 0x4, 0           btfss       PORTA,P_WAIT
000a5a   d7fe     bra	0xa58                    bra         mem_write_wait_loop
                                               ;release IORQ and RD
000a5c   828d     bsf	0x8d, 0x1, 0             bsf         LATE,P_WR
000a5e   8489     bsf	0x89, 0x2, 0             bsf         LATA,P_MREQ
000a60   6895     setf	0x95, 0                 setf        TRISD           ; stop driving the bus again
000a62   0012     return	0                     return
                                           
                                           
                                           ; == Export these refs ========================================================
                                           
                                               GLOBAL      HI_ADDR
                                               GLOBAL      LO_ADDR
                                               GLOBAL      DREG
                                           
                                               GLOBAL      get_reset
                                               GLOBAL      get_dma
                                               GLOBAL      get_slave
                                               GLOBAL      ensure_master
                                               GLOBAL      revert_master
                                               GLOBAL      io_read
                                               GLOBAL      io_write
                                               GLOBAL      mem_read
                                               GLOBAL      mem_write
                                           end
                                           ;==============================================================================
                                           ;    boot.asm
                                           ;==============================================================================
                                           
                                           ;------------------------------------------------------------------------------
                                           ;
                                           ; Simple Debug Kernel for CPU supervisor on Z80 Project Mark 2
                                           ; File Version 2 - 17 Oct 09
                                           ; hairymnstr@gmail.com
