00079e   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 240
0007a0   f004
0007a2   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 241
0007a4   f004
0007a6   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 242
0007a8   f004
0007aa   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 243
0007ac   f004
0007ae   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 244
0007b0   f004
0007b2   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 245
0007b4   f004
0007b6   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 246
0007b8   f004
0007ba   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 247
0007bc   f004
0007be   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 248
0007c0   f004
0007c2   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 249
0007c4   f004
0007c6   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 250
0007c8   f004
0007ca   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 251
0007cc   f004
0007ce   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 252
0007d0   f004
0007d2   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 253
0007d4   f004
0007d6   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 254
0007d8   f004
0007da   ef6e     goto	0x8dc                   goto        serial_error_do_unknown         ; 255
0007dc   f004
                                           
                                           serial_do_get_reset
                                               ; run the get_reset subroutine and return success
0007de   eccc     call	0x998, 0                call        get_reset
0007e0   f004
0007e2   ec06     call	0x80c, 0                call        serial_do_ack
0007e4   f004
0007e6   efa5     goto	0x94a                   goto        serial_handler_exit
0007e8   f004
                                           
                                           serial_do_get_dma
0007ea   ecc4     call	0x988, 0                call        get_dma
0007ec   f004
0007ee   ec06     call	0x80c, 0                call        serial_do_ack
0007f0   f004
0007f2   efa5     goto	0x94a                   goto        serial_handler_exit
0007f4   f004
                                           
                                           serial_do_get_slave
0007f6   ecda     call	0x9b4, 0                call        get_slave
0007f8   f004
0007fa   ec06     call	0x80c, 0                call        serial_do_ack
0007fc   f004
0007fe   efa5     goto	0x94a                   goto        serial_handler_exit
000800   f004
                                           
                                           serial_do_reset
                                               ; special case, send ack then perform a CPU reset
000802   ec06     call	0x80c, 0                call        serial_do_ack
000804   f004
                                           
                                           serial_do_reset_loop
                                               ; need to wait here until the UART has finished sending the two bytes
                                               ; that are in the shift register and transmit buffer
000806   a2ac     btfss	0xac, 0x1, 0           btfss       TXSTA,TRMT
000808   d7fe     bra	0x806                    bra         serial_do_reset_loop
                                               ; TSR is now empty, can safely do reset.
                                           
00080a   00ff     reset                        reset
                                           
                                           serial_do_ack
00080c   6ae1     clrf	0xe1, 0                 clrf        FSR1L
00080e   0e09     movlw	0x9                    movlw       0x09
000810   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000812   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                           
000814   0e01     movlw	0x1                    movlw       0x01
000816   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000818   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
00081a   0e02     movlw	0x2                    movlw       0x02
00081c   6ee9     movwf	0xe9, 0                movwf       FSR0L
00081e   50ef     movf	0xef, 0, 0              movf        INDF0,w
000820   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000822   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
000824   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
000826   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
000828   ec3a     call	0x74, 0                 call        serial_tx_send_packet
00082a   f000
                                           
00082c   0012     return	0                     return
                                           
                                           ; -- serial error functions ---------------------------------------------------
                                           
                                           serial_error_checksum
                                               ; report a message received with a bad checksum
                                               ; special first character because we don't know if the command was corrupt
                                               ; or data following it so start with a "0"
                                               ; set the send pointer to the start of the buffer
00082e   6ae1     clrf	0xe1, 0                 clrf        FSR1L
000830   0e80     movlw	0x80                   movlw       0x80
000832   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000834   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                               ; next is length byte.  checksum fail message is always 3 bytes
000836   0e03     movlw	0x3                    movlw       0x03
000838   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00083a   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now is the error code.  checksum fail = 0x0001
00083c   0e00     movlw	0                      movlw       0x00
00083e   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000840   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
000842   0e01     movlw	0x1                    movlw       0x01
000844   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000846   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now send the calculated checksum for debugging
000848   5183     movf	0x83, 0, 0x1            movf        RX_CHECKSUM,w
00084a   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00084c   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now add the checksum
00084e   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
000850   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                               
                                               ; packet assembled, send the packet
000852   ec3a     call	0x74, 0                 call        serial_tx_send_packet
000854   f000
                                           
000856   efa5     goto	0x94a                   goto        serial_handler_exit
000858   f004
                                           
                                           serial_error_bad_command
                                               ; report a bad command i.e. byte outside [A-Z]
                                               ; set the send pointer to the start of the buffer
00085a   6ae1     clrf	0xe1, 0                 clrf        FSR1L
00085c   0e81     movlw	0x81                   movlw       0x81     ; bad command so can't do the normal upper/lower conversion
00085e   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000860   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                               ; next is length byte.  always 3 bytes for bad command
000862   0e03     movlw	0x3                    movlw       0x03
000864   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000866   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now is the error code.  bad command = 0x0002
000868   0e00     movlw	0                      movlw       0x00
00086a   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00086c   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
00086e   0e02     movlw	0x2                    movlw       0x02
000870   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000872   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now send the bad command for debug
000874   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
000876   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000878   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; finally add the checksum
00087a   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
00087c   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
                                               ; packet assembled, send the packet
00087e   ec3a     call	0x74, 0                 call        serial_tx_send_packet
000880   f000
                                           
000882   efa5     goto	0x94a                   goto        serial_handler_exit
000884   f004
                                           
                                           serial_error_unused_command
                                               ; valid command letter but not used
                                               ; set the send pointer to the start of the buffer
000886   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                               ; command code is same with bit 6 set
000888   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
00088a   8ce8     bsf	0xe8, 0x6, 0             bsf         WREG,6          ; set bit 6, the error flag
00088c   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00088e   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                               ; now the length only the error code now
000890   0e02     movlw	0x2                    movlw       0x02
000892   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000894   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now the error code 0x0003 for unused command
000896   0e00     movlw	0                      movlw       0x00
000898   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00089a   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
00089c   0e03     movlw	0x3                    movlw       0x03
00089e   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008a0   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now the checksum
0008a2   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
0008a4   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
0008a6   ec3a     call	0x74, 0                 call        serial_tx_send_packet
0008a8   f000
                                           
0008aa   efa5     goto	0x94a                   goto        serial_handler_exit
0008ac   f004
                                           
                                           serial_error_wrong_parameters
                                               ; valid command but got the wrong number of parameters
0008ae   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                               ; command code with bit 6 set
0008b0   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
0008b2   8ce8     bsf	0xe8, 0x6, 0             bsf         WREG,6
0008b4   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008b6   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                               ; now the length, error code plus the expected number of params
0008b8   0e03     movlw	0x3                    movlw       0x03
0008ba   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008bc   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now the error code 0x0004
0008be   0e00     movlw	0                      movlw       0x00
0008c0   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008c2   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
0008c4   0e04     movlw	0x4                    movlw       0x04
0008c6   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008c8   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now the correct number of argument bytes
0008ca   5188     movf	0x88, 0, 0x1            movf        MAIN_TEMP,w
0008cc   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008ce   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                               ; now the checksum
0008d0   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
0008d2   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
0008d4   ec3a     call	0x74, 0                 call        serial_tx_send_packet
0008d6   f000
                                           
0008d8   efa5     goto	0x94a                   goto        serial_handler_exit
0008da   f004
                                           
                                           serial_error_do_unknown
0008dc   6ae1     clrf	0xe1, 0                 clrf        FSR1L
0008de   0e49     movlw	0x49                   movlw       0x49
0008e0   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008e2   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                           
0008e4   0e03     movlw	0x3                    movlw       0x03
0008e6   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008e8   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
0008ea   0e00     movlw	0                      movlw       0x00
0008ec   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008ee   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
0008f0   0e09     movlw	0x9                    movlw       0x09
0008f2   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008f4   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
0008f6   0e02     movlw	0x2                    movlw       0x02
0008f8   6ee9     movwf	0xe9, 0                movwf       FSR0L
0008fa   50ef     movf	0xef, 0, 0              movf        INDF0,w
0008fc   6ee6     movwf	0xe6, 0                movwf       POSTINC1
0008fe   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
000900   5185     movf	0x85, 0, 0x1            movf        TX_CHECKSUM,w
000902   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
000904   ec3a     call	0x74, 0                 call        serial_tx_send_packet
000906   f000
                                           
000908   efa5     goto	0x94a                   goto        serial_handler_exit
00090a   f004
                                           
                                           serial_error_packet_length
00090c   0e05     movlw	0x5                    movlw       0x05
00090e   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP
000910   ef92     goto	0x924                   goto        serial_error_generic
000912   f004
                                           
                                           serial_error_bios_address_space
000914   0e06     movlw	0x6                    movlw       0x06
000916   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP
000918   ef92     goto	0x924                   goto        serial_error_generic
00091a   f004
                                           
                                           serial_error_bios_address_offset
00091c   0e07     movlw	0x7                    movlw       0x07
00091e   6f88     movwf	0x88, 0x1              movwf       MAIN_TEMP
000920   ef92     goto	0x924                   goto        serial_error_generic
000922   f004
                                           
                                           serial_error_generic
000924   6ae1     clrf	0xe1, 0                 clrf        FSR1L
000926   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
000928   8ce8     bsf	0xe8, 0x6, 0             bsf         WREG,6
00092a   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00092c   6f85     movwf	0x85, 0x1              movwf       TX_CHECKSUM
                                           
00092e   0e02     movlw	0x2                    movlw       0x02
000930   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000932   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
000934   0e00     movlw	0                      movlw       0x00
000936   6ee6     movwf	0xe6, 0                movwf       POSTINC1
000938   1b85     xorwf	0x85, 0x1, 0x1         xorwf       TX_CHECKSUM,f
                                           
00093a   5188     movf	0x88, 0, 0x1            movf        MAIN_TEMP,w
00093c   6ee6     movwf	0xe6, 0                movwf       POSTINC1
00093e   1985     xorwf	0x85, 0, 0x1           xorwf       TX_CHECKSUM,w
                                           
000940   6ee6     movwf	0xe6, 0                movwf       POSTINC1
                                           
000942   ec3a     call	0x74, 0                 call        serial_tx_send_packet
000944   f000
                                           
000946   efa5     goto	0x94a                   goto        serial_handler_exit
000948   f004
                                           
                                           serial_handler_exit
                                               ; standard exit for all serial response functions
00094a   6ae9     clrf	0xe9, 0                 clrf        FSR0L           ; reset RX pointer
00094c   6b84     clrf	0x84, 0x1               clrf        RX_FLAGS        ; get rid of any flags from this message
00094e   6b80     clrf	0x80, 0x1               clrf        RX_MODE         ; go back to waiting
000950   0012     return	0                     return
                                           
                                           ; == Export Direct Access to ==================================================
                                           
                                               GLOBAL      serial_init
                                               GLOBAL      serial_rx_int
                                               GLOBAL      serial_command_dispatch
                                           
                                           end
                                           ;==============================================================================
                                           ;    main.asm
                                           ;==============================================================================
                                           
