                                           ;== Serial Console Functions ==================================================
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* serial_init - set up the serial port for PC comms                          *
                                           ;*                                                                            *
                                           ;* Inputs:  NONE                                                              *
                                           ;* Outputs: NONE                                                              *
                                           ;* Called:                                                                    *
                                           ;* Changes: SPBRG, RCSTA, TXSTA, BAUDCON, TRISC, RCON, PIR1, PIE1, INTCON     *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           serial_init
000036   6b84     clrf	0x84, 0x1               clrf        RX_FLAGS
000038   0e01     movlw	0x1                    movlw       0x01
00003a   6eea     movwf	0xea, 0                movwf       FSR0H           ; set FSR0 to BANK1, RX Buffer
00003c   6ae9     clrf	0xe9, 0                 clrf        FSR0L
00003e   0e02     movlw	0x2                    movlw       0x02
000040   6ee2     movwf	0xe2, 0                movwf       FSR1H           ; set FSR1 to BANK2, TX Buffer
000042   6ae1     clrf	0xe1, 0                 clrf        FSR1L
000044   0e00     movlw	0                      movlw       RX_COM
000046   6f80     movwf	0x80, 0x1              movwf       RX_MODE         ; set RX_MODE to look for a command
                                               ; first make sure that the RX and TX are both set input, hardware changes
                                               ; this once the UART is enabled
000048   0ec0     movlw	0xc0                   movlw       b'11000000'
00004a   1294     iorwf	0x94, 0x1, 0           iorwf       TRISC,f
                                           
                                               ; next set the UART up
00004c   0e10     movlw	0x10                   movlw       0x10
00004e   6eaf     movwf	0xaf, 0                movwf       SPBRG
000050   0e04     movlw	0x4                    movlw       0x04
000052   6eb0     movwf	0xb0, 0                movwf       SPBRGH          ; 9600 baud high speed 16 bit mode
000054   0e24     movlw	0x24                   movlw       b'00100100'     ; enable sending, 8-bit, high speed baud rate
000056   6eac     movwf	0xac, 0                movwf       TXSTA
000058   0e10     movlw	0x10                   movlw       b'00010000'     ; enable receiving 8-bit
00005a   6eab     movwf	0xab, 0                movwf       RCSTA
00005c   0e08     movlw	0x8                    movlw       b'00001000'     ; no-auto sensing - 16 bit baud rate
00005e   6eb8     movwf	0xb8, 0                movwf       BAUDCON
000060   8eab     bsf	0xab, 0x7, 0             bsf         RCSTA,SPEN      ; enable the serial port
                                           
                                               ; set up interrupt on receive
000062   8ed0     bsf	0xd0, 0x7, 0             bsf         RCON, IPEN
000064   9a9e     bcf	0x9e, 0x5, 0             bcf         PIR1, RCIF
000066   8a9d     bsf	0x9d, 0x5, 0             bsf         PIE1, RCIE
000068   8ef2     bsf	0xf2, 0x7, 0             bsf         INTCON, GIEH    ; enable global interrupts
00006a   0012     return	0                     return
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* serial_send - send the contents of W via the UART                          *
                                           ;*                                                                            *
                                           ;* Inputs:  Byte to send in W                                                 *
                                           ;* Outputs: NONE                                                              *
                                           ;* Called:                                                                    *
                                           ;* Changes: TXREG                                                             *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           serial_send
00006c   a89e     btfss	0x9e, 0x4, 0           btfss PIR1,TXIF     ;test transmit interrupt flag
00006e   d7fe     bra	0x6c                     bra serial_send     ;if clear, TXREG is full
000070   6ead     movwf	0xad, 0                movwf TXREG         ;if set, TXREG is empty, so copy data into it
000072   0012     return	0                     return
                                           
                                           ; -- serial_tx_send_packet ----------------------------------------------------
                                           
                                           serial_tx_send_packet
                                               ; send a whole message packet from the TX Buffer
                                               ; first set the TX pointer to the start of the buffer
000074   6ae1     clrf	0xe1, 0                 clrf        FSR1L
                                               ; send the first byte (the command code)
000076   50e6     movf	0xe6, 0, 0              movf        POSTINC1,w
000078   ec36     call	0x6c, 0                 call        serial_send
00007a   f000
                                               ; now send the length byte, and initialise the counter
00007c   50e6     movf	0xe6, 0, 0              movf        POSTINC1,w
00007e   ec36     call	0x6c, 0                 call        serial_send
000080   f000
000082   6f86     movwf	0x86, 0x1              movwf       TX_COUNT
                                           
000084   0bff     andlw	0xff                   andlw       0xFF
000086   e005     bz	0x92                      bz          serial_tx_send_packet_checksum  ; if length is zero skip data loop
                                               ; now we loop over send and decrement the counter
                                           serial_tx_send_packet_data_loop
000088   50e6     movf	0xe6, 0, 0              movf        POSTINC1,w
00008a   ec36     call	0x6c, 0                 call        serial_send
00008c   f000
00008e   2f86     decfsz	0x86, 0x1, 0x1        decfsz      TX_COUNT,f
000090   d7fb     bra	0x88                     bra         serial_tx_send_packet_data_loop
                                               
                                           serial_tx_send_packet_checksum
000092   9a8b     bcf	0x8b, 0x5, 0             bcf         LATC,5
                                               ; lastly send the checksum
000094   50e6     movf	0xe6, 0, 0              movf        POSTINC1,w
000096   ec36     call	0x6c, 0                 call        serial_send
000098   f000
                                           
                                               ; done, return
00009a   0012     return	0                     return
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;* serial_rx_int - UART receive interrupt routine                             *
                                           ;*                                                                            *
                                           ;* Inputs:  None                                                              *
                                           ;* Outputs: None                                                              *
                                           ;* Called:                                                                    *
                                           ;* Changes: PIR1, w                                                           *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           ; called later but must be on before the page to avoid relative branch errors
                                           ; !! deprecated !!
                                           ; serial_nak_exit
                                           ;     movlw       0x15
                                           ;     call        serial_send
                                           ;     return
                                           
                                           serial_rx_int_error
00009c   98ab     bcf	0xab, 0x4, 0             bcf         RCSTA,CREN              ;clear the CREN bit to clear errors
00009e   0e15     movlw	0x15                   movlw       0x15                    ;prepare a NAK symbol
0000a0   ec36     call	0x6c, 0                 call        serial_send             ;and send it
0000a2   f000
0000a4   88ab     bsf	0xab, 0x4, 0             bsf         RCSTA,CREN              ;re-enable receiving
0000a6   50ae     movf	0xae, 0, 0              movf        RCREG,w                 ;have to read the RCREG to clear the interrupt
0000a8   0012     return	0                     return
                                           
                                           serial_rx_int
0000aa   0e06     movlw	0x6                    movlw       b'00000110'
0000ac   14ab     andwf	0xab, 0, 0             andwf       RCSTA,w                 ;test if either of the error bits are set
0000ae   e1f6     bnz	0x9c                     bnz         serial_rx_int_error     ;if result is not zero an error occurred
                                           serial_rx_int_no_error                  ;if no error occurred, just echo input
                                           
                                               ; if RX_MODE & FC != 0 -> error message, busy
0000b0   0efc     movlw	0xfc                   movlw       0xFC                    ; three non-busy modes
0000b2   1580     andwf	0x80, 0, 0x1           andwf       RX_MODE,w               ; don't change the RX_MODE
0000b4   e1f3     bnz	0x9c                     bnz         serial_rx_int_error     ; busy mode so reply NAK, we don't want data at the moment
                                           
0000b6   0e00     movlw	0                      movlw       UPPER serial_rx_jump_table
0000b8   6efb     movwf	0xfb, 0                movwf       PCLATU
0000ba   0e00     movlw	0                      movlw       HIGH serial_rx_jump_table
0000bc   6efa     movwf	0xfa, 0                movwf       PCLATH
0000be   5180     movf	0x80, 0, 0x1            movf        RX_MODE,w
0000c0   46e8     rlncf	0xe8, 0x1, 0           rlncf       WREG,f
0000c2   46e8     rlncf	0xe8, 0x1, 0           rlncf       WREG,f
0000c4   ef64     goto	0xc8                    goto        serial_rx_jump_table
0000c6   f000
                                           
                                               PAGE            ; doing a jump table need to make sure it's all in one page of memory
                                           
                                           serial_rx_jump_table
0000c8   26f9     addwf	0xf9, 0x1, 0           addwf       PCL,f
0000ca   ef6d     goto	0xda                    goto        serial_rx_command       ;0
0000cc   f000
0000ce   ef73     goto	0xe6                    goto        serial_rx_length        ;1
0000d0   f000
0000d2   ef7d     goto	0xfa                    goto        serial_rx_data          ;2
0000d4   f000
0000d6   ef84     goto	0x108                   goto        serial_rx_checksum      ;3
0000d8   f000
                                           
                                           ;command byte
                                           serial_rx_command
                                               ; store the command
0000da   50ae     movf	0xae, 0, 0              movf        RCREG,w
0000dc   6f81     movwf	0x81, 0x1              movwf       RX_COMMAND    
                                               ; put it in the RX buffer
0000de   6eee     movwf	0xee, 0                movwf       POSTINC0
                                               ; also store it to start the checksum
0000e0   6f83     movwf	0x83, 0x1              movwf       RX_CHECKSUM
                                               ; change to length mode
0000e2   2b80     incf	0x80, 0x1, 0x1          incf        RX_MODE,f
                                               ; return
0000e4   0012     return	0                     return
                                           
                                           ; length byte
                                           serial_rx_length
                                               ; set the byte count
0000e6   50ae     movf	0xae, 0, 0              movf        RCREG,w
0000e8   6f82     movwf	0x82, 0x1              movwf       RX_COUNT
                                               ; store this in the RX buffer too
0000ea   6eee     movwf	0xee, 0                movwf       POSTINC0
                                               ; xor the length for checksum
0000ec   1b83     xorwf	0x83, 0x1, 0x1         xorwf       RX_CHECKSUM,f
                                               ; change to data mode
0000ee   2b80     incf	0x80, 0x1, 0x1          incf        RX_MODE,f
                                               ; if the byte count is 0 skip data mode
0000f0   0e00     movlw	0                      movlw       0x00
0000f2   6382     cpfseq	0x82, 0x1             cpfseq      RX_COUNT
0000f4   0012     return	0                     return
0000f6   2b80     incf	0x80, 0x1, 0x1          incf        RX_MODE,f       ; inc to checksum mode
0000f8   0012     return	0                     return
                                           
                                           serial_rx_data
                                               ; store the data byte in the indirect register
0000fa   50ae     movf	0xae, 0, 0              movf        RCREG,w
0000fc   6eee     movwf	0xee, 0                movwf       POSTINC0
                                               ; update the checksum
0000fe   1b83     xorwf	0x83, 0x1, 0x1         xorwf       RX_CHECKSUM,f
                                               ; decrement the byte counter
000100   2f82     decfsz	0x82, 0x1, 0x1        decfsz      RX_COUNT,f
000102   0012     return	0                     return
                                               ; if execution got here the full count of bytes is done
000104   2b80     incf	0x80, 0x1, 0x1          incf        RX_MODE,f       ; inc to checksum mode
000106   0012     return	0                     return
                                           
                                           serial_rx_checksum
000108   50ae     movf	0xae, 0, 0              movf        RCREG,w
                                               ; keep a copy in the RX buffer
00010a   6eee     movwf	0xee, 0                movwf       POSTINC0
                                               ; do an xor with the calculated checksum so far, should result in zero
00010c   1b83     xorwf	0x83, 0x1, 0x1         xorwf       RX_CHECKSUM,f
00010e   e005     bz	0x11a                     bz          serial_rx_checksum_passed
                                               ; if execution got here the checksum failed
000110   1b83     xorwf	0x83, 0x1, 0x1         xorwf       RX_CHECKSUM,f           ; return to the calculated checksum for debug
000112   8384     bsf	0x84, 0x1, 0x1           bsf         RX_FLAGS,CHECKSUM_FAIL  ; set a flag
000114   2b80     incf	0x80, 0x1, 0x1          incf        RX_MODE,f               ; switch to busy mode
000116   6ae9     clrf	0xe9, 0                 clrf        FSR0L                   ; reset the RX pointer
000118   0012     return	0                     return
                                           
                                           serial_rx_checksum_passed
                                               ; checksum okay, set busy mode and exit
00011a   2b80     incf	0x80, 0x1, 0x1          incf        RX_MODE,f
00011c   6ae9     clrf	0xe9, 0                 clrf        FSR0L
00011e   0012     return	0                     return
                                           
                                           ; -- End of interrupt routines ------------------------------------------------
                                           
                                           ;******************************************************************************
                                           ;*                                                                            *
                                           ;*      serial_command_dispatch - checks command and branches to appropriate  *
                                           ;*                                routine                                     *
                                           ;*                                                                            *
                                           ;*      Called from the main loop to deal with any complete commands          *
                                           ;*                                                                            *
                                           ;******************************************************************************
                                           
                                           serial_command_dispatch
                                               ; first check if there is a command
000120   a580     btfss	0x80, 0x2, 0x1         btfss       RX_MODE,2       ; if bit 2 is set (mode 4 == RX_BUSY) there is a command ready
000122   0012     return	0                     return                      ; otherwise nothing to do here
                                               ; next see if there was an error in the checksum
000124   b384     btfsc	0x84, 0x1, 0x1         btfsc       RX_FLAGS,CHECKSUM_FAIL
000126   ef17     goto	0x82e                   goto        serial_error_checksum           ; if there was report the error
000128   f004
                                               ; no error, so check the command is within bounds
00012a   0ee0     movlw	0xe0                   movlw       0xE0
00012c   1581     andwf	0x81, 0, 0x1           andwf       RX_COMMAND,w
00012e   a4d8     btfss	0xd8, 0x2, 0           btfss       STATUS,Z
000130   ef2d     goto	0x85a                   goto        serial_error_bad_command        ; command was more than 31
000132   f004
                                               ; We know RX_COMMAND is a valid command now.  Jump
000134   0e00     movlw	0                      movlw       UPPER serial_command_jump_table
000136   6efb     movwf	0xfb, 0                movwf       PCLATU
000138   0e01     movlw	0x1                    movlw       HIGH serial_command_jump_table
00013a   6efa     movwf	0xfa, 0                movwf       PCLATH
00013c   5181     movf	0x81, 0, 0x1            movf        RX_COMMAND,w
00013e   46e8     rlncf	0xe8, 0x1, 0           rlncf       WREG,f
000140   46e8     rlncf	0xe8, 0x1, 0           rlncf       WREG,f
000142   efa3     goto	0x146                   goto        serial_command_jump_table
